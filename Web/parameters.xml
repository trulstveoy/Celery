    <parameterEntry kind="XmlFile" scope="\\web.config$" match="/configuration/appSettings/add[@key='GoogleSecret']/@value" />
  </parameter>
  <parameter name="MongoUri">
    <parameterEntry kind="XmlFile" scope="\\web.config$" match="/configuration/appSettings/add[@key='MongoUri']/@value" />
